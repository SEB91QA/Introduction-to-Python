def square_root(x):
    '''Calculate the square root number'''
    return x**0.5


square_root(6.25)


def maximum(value1, value2, value3):
    '''Returns the highest value'''
    max_value = value1
    if value2 > max_value:
        max_value = value2
    if value3 > max_value:
        max_value = value3
    return max_value


maximum(12, 27, 36)


maximum(12.3, 45.6, 9.7)


maximum('yellow', 'red', 'orange')


maximum(13.5, -3, 7)


max('yellow', 'red', 'orange', 'blue', 'green')


